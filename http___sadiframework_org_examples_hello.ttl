@prefix mygrid:  <http://www.mygrid.org.uk/mygrid-moby-service#> .
@prefix protege-dc:  <http://protege.stanford.edu/plugins/owl/dc/protege-dc.owl#> .

<http://sadiframework.org/examples/hello>
      a       mygrid:serviceDescription ;
      <http://www.w3.org/2000/01/rdf-schema#comment>
              "A simple \"Hello, World\" service that reads a name and attaches a greeting."^^<http://www.w3.org/2001/XMLSchema#string> ;
      <http://www.w3.org/2000/01/rdf-schema#label>
              "Hello, world"^^<http://www.w3.org/2001/XMLSchema#string> ;
      mygrid:hasOperation
              [ a       mygrid:operation ;
                mygrid:hasUnitTest
                        [ a       mygrid:unitTest ;
                          mygrid:exampleInput <http://sadiframework.org/examples/t/hello.input.1.rdf> ;
                          mygrid:exampleOutput
                                  <http://sadiframework.org/examples/t/hello.output.1.rdf>
                        ] ;
                mygrid:inputParameter
                        [ a       mygrid:parameter ;
                          mygrid:objectType <http://sadiframework.org/examples/hello.owl#NamedIndividual>
                        ] ;
                mygrid:outputParameter
                        [ a       mygrid:parameter ;
                          mygrid:objectType <http://sadiframework.org/examples/hello.owl#GreetedIndividual>
                        ]
              ] ;
      mygrid:hasServiceDescriptionText
              "A simple \"Hello, World\" service that reads a name and attaches a greeting."^^<http://www.w3.org/2001/XMLSchema#string> ;
      mygrid:hasServiceNameText
              "Hello, world"^^<http://www.w3.org/2001/XMLSchema#string> ;
      mygrid:providedBy
              [ a       mygrid:organisation ;
                protege-dc:creator "info@sadiframework.org"^^<http://www.w3.org/2001/XMLSchema#string> ;
                mygrid:authoritative
                        "true"^^<http://www.w3.org/2001/XMLSchema#boolean>
              ] .
